{
  "name": "anime-apollo-graphql",
  "homepage": "https://kentandrian.github.io/anime-apollo-graphql",
  "version": "0.4.13",
  "private": true,
  "type": "module",
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "schema:download": "npx apollo service:download --endpoint=https://anilist.co/graphql ./src/app/graphql/graphql-schema.json",
    "schema:generate-watch": "npx apollo codegen:generate --localSchemaFile=./src/app/graphql/graphql-schema.json --target=typescript --tagName=gql --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@apollo/client": "^3.9.1",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@reduxjs/toolkit": "^2.1.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "redux-logger": "^3.0.6",
    "reselect": "^5.1.0",
    "styled-components": "^6.1.8",
    "tailwindcss": "^3.4.1",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.1",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.49",
    "@types/react-dom": "^18.2.18",
    "@types/redux-logger": "^3.0.13",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "gh-pages": "^6.1.1",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.33",
    "rollup": "4.9.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2"
  },
  "peerDependencies": {
    "esbuild": "^0.19.12"
  }
}
