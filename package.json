{
  "name": "anime-apollo-graphql",
  "homepage": "https://kentandrian.github.io/anime-apollo-graphql",
  "version": "0.2.14",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.8.4",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@reduxjs/toolkit": "^1.9.6",
    "@svgr/webpack": "^8.0.1",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-scripts": "5.0.1",
    "redux-logger": "^3.0.6",
    "reselect": "^4.1.8",
    "styled-components": "^6.0.8",
    "tailwindcss": "^3.3.3",
    "web-vitals": "^3.4.0"
  },
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "schema:download": "npx apollo service:download --endpoint=https://anilist.co/graphql ./src/app/graphql/graphql-schema.json",
    "schema:generate-watch": "npx apollo codegen:generate --localSchemaFile=./src/app/graphql/graphql-schema.json --target=typescript --tagName=gql --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/dom4": "^2.0.2",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@types/redux-logger": "^3.0.9",
    "@types/styled-components": "^5.1.27",
    "gh-pages": "^6.0.0",
    "postcss": "^8.4.30",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "react-scripts/@svgr/webpack": "^8.0.1"
  }
}
