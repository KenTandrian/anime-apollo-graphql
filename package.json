{
  "name": "anime-apollo-graphql",
  "homepage": "https://kentandrian.github.io/anime-apollo-graphql",
  "version": "0.3.71",
  "private": true,
  "type": "module",
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "schema:download": "npx apollo service:download --endpoint=https://anilist.co/graphql ./src/app/graphql/graphql-schema.json",
    "schema:generate-watch": "npx apollo codegen:generate --localSchemaFile=./src/app/graphql/graphql-schema.json --target=typescript --tagName=gql --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "@reduxjs/toolkit": "^2.0.1",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "redux-logger": "^3.0.6",
    "reselect": "^5.0.1",
    "styled-components": "^6.1.6",
    "tailwindcss": "^3.4.1",
    "web-vitals": "^3.5.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/redux-logger": "^3.0.12",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "gh-pages": "^6.1.1",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.33",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.2"
  }
}
